<table>
  <tr>
    <th>User Name</th>
    <th>Email</th>
  </tr>
<%User.all.each do |u|%>
   <tr>
     <td> <%= u.username %></td>
     <td> <%= u.email%></td>

     <td> <%= link_to 'Display', user_path(u)%></td>
     <td> <%= link_to 'Edit', edit_user_path(u) %></td>
     <td> <%= link_to 'Delete' , user_path(u), method: :delete, data: {confirm: 'Are you suire you want to delete this user?'} %> </td>
   </tr>
<%end%>
</table>
